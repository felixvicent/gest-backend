generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @default(uuid())
  name       String
  email      String   @unique
  password   String ///@encrypted
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("users")
}

model Category {
  id         String   @id @default(uuid())
  label      String   @unique
  type       String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("categories")
}
